{
	"name": "y-durablestream",
	"version": "0.1.0",
	"description": "Yjs document synchronization between Cloudflare Durable Objects via TransformStream with pluggable storage backends",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"license": "MIT",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sammccord/y-durablestream.git"
	},
	"homepage": "https://github.com/sammccord/y-durablestream#readme",
	"bugs": {
		"url": "https://github.com/sammccord/y-durablestream/issues"
	},
	"author": "Sam McCord",
	"keywords": [
		"yjs",
		"cloudflare",
		"cloudflare-workers",
		"durable-objects",
		"crdt",
		"sync",
		"transformstream",
		"realtime",
		"collaboration",
		"rpc"
	],
	"scripts": {
		"build": "tsup",
		"typecheck": "tsc --noEmit",
		"test": "vitest run --passWithNoTests",
		"test:watch": "vitest --passWithNoTests",
		"version": "changeset version",
		"release": "changeset publish"
	},
	"dependencies": {
		"lib0": "^0.2.104",
		"y-protocols": "^1.0.6",
		"yjs": "^13.6.24"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.2",
		"@cloudflare/vitest-pool-workers": "^0.12.10",
		"@types/node": "^25.2.1",
		"tsup": "^8.4.0",
		"typescript": "^5.8.3",
		"vitest": "~3.2.0",
		"wrangler": "^4.63.0"
	}
}
